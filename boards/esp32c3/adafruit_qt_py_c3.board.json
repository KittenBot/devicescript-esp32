{
    "$schema": "../esp32deviceconfig.schema.json",
    "devName": "Adafruit QT Py ESP32-C3 WiFi",
    "productId": "0x3693d40b",
    "$description": "A tiny ESP32-C3 board.",
    "url": "https://www.adafruit.com/product/5405",
    "log": {
        "pinTX": "TX_D6"
    },
    "led": {
        "type": 1,
        "pin": 2
    },
    "i2c": {
        "$connector": "Qwiic",
        "pinSCL": "SCL_D5",
        "pinSDA": "SDA_D4"
    },
    "pins": {
        "A0_D0": 4,
        "A1_D1": 3,
        "A2_D2": 1,
        "A3_D3": 0,
        "SDA_D4": 5,
        "SCL_D5": 6,
        "TX_D6": 21,
        "RX_D7": 20,
        "SCK_D8": 10,
        "MISO_D9": 8,
        "MOSI_D10": 7
    },
    "$services": [
        {
            "service": "button",
            "name": "buttonBOOT",
            "pin": 9
        }
    ]
}